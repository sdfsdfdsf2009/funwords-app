{
  "name": "video-generation-workstation",
  "version": "1.0.0",
  "description": "A comprehensive video generation workstation with AI integration",
  "main": "index.js",
  "scripts": {
    "dev": "next dev -p 3003",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=src",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:navigation": "node tests/test-runner.js",
    "test:ui": "node tests/test-runner.js --category=UI_UX",
    "test:performance": "node tests/test-runner.js --category=PERFORMANCE",
    "test:compatibility": "node tests/test-runner.js --category=COMPATIBILITY",
    "test:ux": "node tests/test-runner.js --category=USER_EXPERIENCE",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:lighthouse": "lighthouse http://localhost:3003 --output=json --output-path=./lighthouse-results.json",
    "test:cross-browser": "node scripts/cross-browser-test.js",
    "test:cross-browser:headed": "HEADLESS=false node scripts/cross-browser-test.js",
    "test:compatibility-report": "node scripts/cross-browser-test.js && open test-results/cross-browser-$(ls -t test-results/cross-browser-*.html | head -n 1 | sed 's/.*cross-browser-\\(.*\\)\\.html/\\1/')",
    "test:quality-gate": "npm run type-check && npm run lint && npm run test:ci && npm run test:e2e",
    "test:button-interactions": "node test-button-interactions.js",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "node -r tsx lib/seed.ts",
    "docker:start": "./scripts/docker-start.sh",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^16.0.3",
    "@prisma/client": "^6.19.0",
    "@remotion/bundler": "^4.0.375",
    "@remotion/cli": "^4.0.375",
    "@remotion/player": "^4.0.375",
    "@remotion/renderer": "^4.0.375",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.0",
    "@tanstack/react-query": "^5.90.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "@types/papaparse": "^5.3.0",
    "@types/react": "^18.0.0",
    "@types/react-beautiful-dnd": "^13.1.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-virtualized": "^9.21.0",
    "@types/uuid": "^9.0.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.3",
    "chrome-launcher": "^1.2.1",
    "clsx": "^2.0.0",
    "critters": "^0.0.23",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "ffmpeg.js": "^4.2.0",
    "immer": "^10.2.0",
    "jsonwebtoken": "^9.0.2",
    "lighthouse": "^13.0.1",
    "lucide-react": "^0.292.0",
    "next": "^14.0.0",
    "next-auth": "^4.24.13",
    "node-fetch": "^2.7.0",
    "papaparse": "^5.4.0",
    "playwright": "^1.56.1",
    "postcss": "^8.4.0",
    "prisma": "^6.19.0",
    "react": "^18.0.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.0.0",
    "react-dropzone": "^14.2.0",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^10.0.0",
    "react-virtualized": "^9.22.0",
    "react-window": "^2.2.3",
    "redis": "^5.9.0",
    "remotion": "^4.0.375",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "uuid": "^9.0.0",
    "wavesurfer.js": "^7.3.0",
    "web-vitals": "^5.1.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.0.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "msw": "^2.12.2"
  },
  "keywords": [
    "video-generation",
    "ai",
    "video-editing",
    "csv-import",
    "media-testing",
    "navigation-testing"
  ],
  "author": "",
  "license": "MIT"
}